{
  "name": "Khoj",
  "id": "khoj",
  "available": true,
  "short_desc": "Your AI second brain - chat with your documents using local or cloud AI models",
  "author": "khoj-ai",
  "port": 42110,
  "categories": ["ai", "utilities", "development"],
  "description": "Khoj is a personal AI app to extend your capabilities, featuring local and cloud AI model support, document chat, and advanced semantic search.",
  "tipi_version": 1,
  "version": "latest",
  "source": "https://github.com/khoj-ai/khoj",
  "exposable": true,
  "supported_architectures": ["amd64", "arm64"],
  "form_fields": [
    {
      "type": "email",
      "label": "Admin Email",
      "required": true,
      "env_variable": "KHOJ_ADMIN_EMAIL"
    },
    {
      "type": "password",
      "label": "Admin Password",
      "min": 8,
      "required": true,
      "env_variable": "KHOJ_ADMIN_PASSWORD"
    },
    {
      "type": "password",
      "label": "Django Secret Key",
      "min": 32,
      "required": true,
      "env_variable": "KHOJ_DJANGO_SECRET_KEY"
    },
    {
      "type": "boolean",
      "label": "Enable Anonymous Mode",
      "required": false,
      "default": false,
      "env_variable": "KHOJ_ANONYMOUS_MODE"
    }
  ]
}